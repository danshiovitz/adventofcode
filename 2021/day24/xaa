inp w    # w = input()
mul x 0  # x = 0
add x z  # x += z
mod x 26 # x %= 26
div z 1  # pass
add x 15 # x += 15
eql x w  # x = (x == w) ? 1 : 0
eql x 0  # x = (x == 0) ? 1 : 0
mul y 0  # y = 0
add y 25 # y += 25
mul y x  # y *= x
add y 1  # y += 1
mul z y  # z *= y
mul y 0  # y = 0
add y w  # y += w
add y 15 # y += 15
mul y x  # y *= x
add z y  # z += y

w = input()
x = z % 26
x += 15
x = (x != w) ? 1 : 0
z *= 25 * x + 1
y = (w + 15) * x
z += y

# for digit 1, z ends up as w + 15
